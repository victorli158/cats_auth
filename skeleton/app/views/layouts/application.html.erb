<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<h3><%= current_user.nil? ? "" : current_user.user_name %></h3>
<% if current_user.nil? %>
  <%= button_to "Login", new_session_url %>
<% else %>
  <%= button_to "Logout", session_url(current_user), method: "delete" %>
<% end %>
<%= yield %>

</body>
</html>
